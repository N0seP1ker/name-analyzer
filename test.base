tuple Icecream {
    integer color.
    logical sold.
}.

tuple Point {
    integer x.
    integer y.
    tuple Icecream blah.
}.

tuple AltPoint {
    tuple Point p.
    integer count.
}.

integer f{integer x, logical b} [ ]

void g{} [
    integer a.
    logical b.
    tuple Point p1.
    tuple AltPoint pPoint.
    p1:x = a.
    b = a == 3.
    f(a + p1:y*2, b).
    g().
    pPoint:p:blah:color = 100.
    pPoint:p:x = 200.
    
]
